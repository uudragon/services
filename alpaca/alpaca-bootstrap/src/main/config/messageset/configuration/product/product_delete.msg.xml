<?xml version="1.0" encoding="UTF-8"?>
<messageset xmlns="http://alpaca.vdrinkup.net/configuration" type="json" 
	from="protocol:bam.channel/product_delete"
	to="flow:product_delete">
	<in>
			<element name="ID" binding="id" type="long" />
    	<element name="PRODUCT_CODE" binding="product_code" type="string" />
	</in>
	<out>
		<element name="success" binding="success" type="boolean"/>
		<element name="errorMessage" binding="errorMessage" type="string"/>
	</out>
</messageset>
