<?xml version="1.0" encoding="UTF-8"?>
<messageset xmlns="http://alpaca.vdrinkup.net/configuration" type="json" 
	from="protocol:bam.channel/product_saved"
	to="flow:product_saved">
	<in>
    	<element name="PRODUCT_CODE" binding="product_code" type="string" />
    	<element name="PRODUCT_NAME" binding="product_name" type="string" />
    	<element name="PRODUCT_DESC" binding="product_desc" type="string" />
    	<element name="CREATER" binding="creater" type="string" />
	</in>
	<out>
		<element name="success" binding="success" type="boolean"/>
		<element name="errorMessage" binding="errorMessage" type="string"/>
	</out>
</messageset>
