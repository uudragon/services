<?xml version="1.0" encoding="UTF-8"?>
<env>
	<!-- 控制端口，用于发送控制命令或关闭应用 -->
	<await.port>9999</await.port>
	<!-- 关闭等待超时时间 -->
	<await.shutdown.timeout>3000</await.shutdown.timeout>
	<!-- 输入端线程池大小 -->
	<input.threadpool.size>50</input.threadpool.size>
	<!-- 输出端线程池大小 -->
	<output.threadpool.size>50</output.threadpool.size>
	<!-- 流程执行超时时间 -->
	<execute.timeout>300000</execute.timeout>
	<!-- 引擎关闭等待时间 -->
	<engine.shutdown.timeout>3000</engine.shutdown.timeout>
</env>
